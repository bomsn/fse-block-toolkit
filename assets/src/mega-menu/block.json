{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fse-block-toolkit/mega-menu",
  "title": "Mega Menu",
  "description": "Display a mega menu.",
  "category": "custom",
  "icon": "smiley",
  "textdomain": "fse-block-toolkit",
  "keywords": [
    "custom",
    "menu"
  ],
  "parent": [
    "core/navigation"
  ],
  "attributes": {
    "label": {
      "type": "string"
    },
    "menuSlug": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "rel": {
      "type": "string"
    },
    "id": {
      "type": "number"
    },
    "opensInNewTab": {
      "type": "boolean",
      "default": false
    },
    "url": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "isTopLevelLink": {
      "type": "boolean"
    }
  },
  "usesContext": [
    "textColor",
    "customTextColor",
    "backgroundColor",
    "customBackgroundColor",
    "overlayTextColor",
    "customOverlayTextColor",
    "overlayBackgroundColor",
    "customOverlayBackgroundColor",
    "fontSize",
    "customFontSize",
    "showSubmenuIcon",
    "style"
  ],
  "supports": {
    "reusable": false,
    "html": false,
    "__experimentalSlashInserter": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    },
    "renaming": false,
    "interactivity": true
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}
